{
  "name": "@integral-x/observability",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/observability/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "libs/observability/dist",
        "main": "libs/observability/src/index.ts",
        "tsConfig": "libs/observability/tsconfig.json"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/observability/**/*.ts"]
      }
    }
  },
  "tags": []
}
